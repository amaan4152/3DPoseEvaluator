{
    "gast": {
        "dirname": "GAST-Net-3DPoseEstimation",
        "env": "gast-env",
        "cmd": "python3 gen_skes.py",
        "vid_flag" : "-v",
        "video_path_type": "name",
        "animation_flag" : "--aniamtion",
        "no_animation_flag" : "",
        "flags": {
            "-v" : "",
            "-np" : 1
        }, 
        "output_dir": "output/",
        "output_dataFormats": "npz",
        "output_videoFormats": "mp4"
    },
    "vibe": {
        "dirname": "VIBE", 
        "env": "vibe-env",
        "cmd": "python3 demo.py",
        "vid_flag" : "--vid_file",
        "video_path_type": "absolute",
        "animation_flag" : "",
        "no_animation_flag" : "--no_render",
        "flags": {
            "--vid_file" : "",
            "--output_folder" : "output/",
            "--no_render" : "",
            "--tracker_batch_size" : 4,
            "--vibe_batch_size" : 128
        },
        "output_dir": "output/",
        "output_dataFormats": "pkl",
        "output_videoFormats": "mp4"
    },
    "blazepose": {
        "dirname": "Blazepose", 
        "env": "",
        "cmd": "python3 exec_blazepose.py",
        "vid_flag" : "-v",
        "video_path_type": "absolute",
        "animation_flag" : "--animate",
        "no_animation_flag" : "",
        "flags": {
            "-v" : ""
        }, 
        "output_dir": "output/",
        "output_dataFormats": "npz",
        "output_videoFormats": "mp4"
    }
}
